{
  "name": "vibe-tutor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server.mjs",
    "android:sync": "npx cap sync android",
    "android:build": "cd android && .\\gradlew.bat assembleDebug",
    "android:build:clean": "cd android && .\\gradlew.bat clean assembleDebug",
    "android:open": "npx cap open android",
    "android:doctor": "npx cap doctor",
    "android:install": "adb install -r android/app/build/outputs/apk/debug/app-debug.apk",
    "android:uninstall": "adb uninstall com.vibetech.tutor",
    "android:logs": "adb logcat | findstr /I capacitor",
    "android:full-build": "npm run build && npx cap sync android && cd android && .\\gradlew.bat clean assembleDebug",
    "android:deploy": "npm run android:full-build && npm run android:uninstall && npm run android:install"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/file-transfer": "^1.0.6",
    "@capacitor/filesystem": "^7.1.4",
    "@google/genai": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "lucide-react": "^0.544.0",
    "music-metadata-browser": "^2.5.11",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
